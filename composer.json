{
	"name": "common-gateway/huwelijksplanner-bundle",
	"description": "A symfony package for dutch municipality marriage functionality",
	"type" : "symfony-bundle",
	"keywords" : [
		"commongateway",
		"common",
		"gateway",
		"conduction",
		"symfony",
		"common-gateway-plugin"
	],
	"homepage" : "https://commongatway.nl",
	"license": "EUPL-1.2",
	"minimum-stability" : "dev",
    "require": {
        "php": ">=7.4",
        "commongateway/corebundle": ">=1.0.79",
        "common-gateway/brp-bundle": ">=0.1.4",
        "common-gateway/klanten-bundle": ">=0.1.3",
        "common-gateway/zgw-bundle": ">=0.1.34",
        "moneyphp/money": "3.3.3",
		"symfony/sendinblue-mailer": ">=5.3.14"
    },
	"require-dev" : {
		"symfony/dependency-injection" : "~3.4|~4.1|~5.0"
	},
	"autoload" : {
		"psr-4" : {
			"CommonGateway\\HuwelijksplannerBundle\\" : "src/"
		}
	},
	"scripts": {
		"post-package-install": "composer require common-gateway/huwelijksplanner-bundle",
		"post-package-update": "composer update common-gateway/huwelijksplanner-bundle",
		"post-package-uninstall": "composer remove common-gateway/huwelijksplanner-bundle"
	}
}
