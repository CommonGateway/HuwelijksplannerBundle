      {
        "reference": "https://hp.nl/action/hp.CreatePaymentAction.action.json",
        "actionHandler": "CommonGateway\\HuwelijksplannerBundle\\ActionHandler\\CreatePaymentHandler",
        "listens": ["huwelijksplanner.get.payment"]
      }
