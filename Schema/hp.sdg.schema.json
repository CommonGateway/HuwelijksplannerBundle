{
    "$id": "https://vng.opencatalogi.nl/schemas/hp.sdg.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "version": "0.1",
    "type": "object",
    "title": "SDG",
    "properties":{
        "url": {
            "type":"string"
        },
        "uuid": {
            "type":"string"
        },
        "upnLabel": {
            "type":"string"
        },
        "upnUri":{
            "type":"string"
        },
        "versie": {
            "type":"integer"
        },
        "publicatieDatum": {
            "type":"string"
        },
        "productAanwezigheid": {
            "type":"boolean"
        },
        "productValtOnder": {
            "type":"string"
        },
        "verantwoordelijkeOrganisatie": {
            "$ref": "#/definitions/SDGLocatie"
        },
        "bevoegdeOrganisatie": {
            "$ref": "#/definitions/SDGLocatie"
        },
        "catalogus": {
            "type":"string"
        },
        "locaties": {
            "type":"array"
        },
        "doelgroep": {
            "type":"string"
        },
        "vertalingen": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/SDGVertaling"
            }
        },
        "gerelateerdeProducten": {
            "type":"array",
            "items": {
                "$ref": "#/definitions/SDGProduct"
            }
        }
    }
}