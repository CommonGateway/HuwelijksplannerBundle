{
    "$id": "https://huwelijksplanner.nl/schemas/mollie.payment.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "version": "0.1",
    "type": "object",
    "title": "Payment",
    "properties":{
        "resource": {
            "type": "string",
            "example": "payment"
        },
        "mode": {
            "type": "string",
            "example": "test"
        },
        "createdAt": {
            "type": "string",
            "example": "2018-03-12T11:51:35+00:00"
        },
        "amount": {
            "type": "object",
            "propeties": {
                "value": {
                    "type": "string",
                    "example": "1.00"
                },
                "currency": {
                    "type": "string",
                    "example": "EUR"
                }
            }
        },
        "description": {
            "type": "string",
            "example": "Order 66"
        },
        "method": {
            "type": "string"
        },
        "metadata": {
            "type": "array"
        },
        "status": {
            "type": "string",
            "example": "open"
        },
        "isCancelable": {
            "type": "bool"
        },
        "expiresAt": {
            "type": "string",
            "example": "2018-03-12T12:06:35+00:00"
        },
        "details": {
            "type": "array"
        },
        "profileId": {
            "type": "string",
            "example": "pfl_7N5qjbu42V"
        },
        "sequenceType": {
            "type": "string",
            "example": "oneoff",
        },
        "redirectUrl": {
            "type": "string",
            "example": "https://www.example.org/payment/completed"
        },
        "_links": {
            "type": "array"
        }
    }

}